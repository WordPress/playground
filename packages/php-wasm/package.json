{
  "name": "@wordpress/php-wasm",
  "version": "0.0.1",
  "description": "",
  "files": [
    "package.json",
    "build"
  ],
  "exports": {
    ".": {
      "import": "./build/web/index.js",
      "default": "./build/web/index.js",
      "types": "./build/web/dts/index.d.ts"
    },
    "./web/service-worker": {
      "import": "./build/web/service-worker.js",
      "types": "./build/web/dts/web/service-worker/index.d.ts"
    },
    "./node": {
      "import": "./build/node/index.js",
      "types": "./build/node/dts/index.d.ts"
    },
    "./node/*": {
      "import": "./build/node/*",
      "types": "./build/node/dts/*"
    }
  },
  "type": "module",
  "types": "build/web/dts/index.d.ts",
  "scripts": {
    "dev": "rollup -c rollup.config.mjs -w",
    "build": "rollup -c rollup.config.mjs",
    "build:php": "npm run build:php:web",
    "build:php:web": "npm-run-all \"build:php:web:*\"",
    "build:php:web:8.2": "PHP_VERSION=8.2.0 node ./src/php-build/build.js",
    "build:php:web:8.1": "PHP_VERSION=8.1.14 node ./src/php-build/build.js",
    "build:php:web:8.0": "PHP_VERSION=8.0.11 node ./src/php-build/build.js",
    "build:php:web:7.4": "PHP_VERSION=7.4.33 node ./src/php-build/build.js",
    "build:php:web:7.3": "PHP_VERSION=7.3.33 node ./src/php-build/build.js",
    "build:php:web:7.2": "PHP_VERSION=7.2.34 node ./src/php-build/build.js",
    "build:php:web:7.1": "PHP_VERSION=7.1.30 node ./src/php-build/build.js",
    "build:php:web:7.0": "PHP_VERSION=7.0.33 node ./src/php-build/build.js",
    "build:php:web:5.6": "PHP_VERSION=5.6.40 node ./src/php-build/build.js",
    "build:php:node": "npm-run-all 'build:php:node:*'",
    "build:php:node:8.2": "PLATFORM=node PHP_VERSION=8.2.0 node ./src/php-build/build.js",
    "build:php:node:8.1": "PLATFORM=node PHP_VERSION=8.1.14 node ./src/php-build/build.js",
    "build:php:node:8.0": "PLATFORM=node PHP_VERSION=8.0.11 node ./src/php-build/build.js",
    "build:php:node:7.4": "PLATFORM=node PHP_VERSION=7.4.33 node ./src/php-build/build.js",
    "build:php:node:7.3": "PLATFORM=node PHP_VERSION=7.3.33 node ./src/php-build/build.js",
    "build:php:node:7.2": "PLATFORM=node PHP_VERSION=7.2.34 node ./src/php-build/build.js",
    "build:php:node:7.1": "PLATFORM=node PHP_VERSION=7.1.30 node ./src/php-build/build.js",
    "build:php:node:7.0": "PLATFORM=node PHP_VERSION=7.0.33 node ./src/php-build/build.js",
    "build:php:node:5.6": "PLATFORM=node PHP_VERSION=5.6.40 node ./src/php-build/build.js",
    "clean": "rm -rf build/*",
    "test": "jest"
  },
  "author": "The WordPress contributors",
  "license": "Apache-2.0",
  "dependencies": {
    "comlink": "^4.4.1",
    "glob": "^9.2.1",
    "rollup-plugin-copy": "^3.4.0"
  },
  "devDependencies": {
    "@rollup/plugin-typescript": "^11.0.0",
    "@rollup/plugin-url": "^8.0.1",
    "@types/node": "12.20.24",
    "npm-run-all": "^4.1.5",
    "rollup": "^3.18.0",
    "typescript": "beta",
    "vite-plugin-static-copy": "^0.13.1"
  }
}
